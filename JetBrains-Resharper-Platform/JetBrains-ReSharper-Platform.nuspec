<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>JetBrains-Resharper-Platform</id>
    <version>2017.2.2</version>
    <title>JetBrains ReSharper Ultimate</title>
    <authors>JetBrains</authors>
    <projectUrl>https://www.jetbrains.com/resharper</projectUrl>
    <description>
      This package installs JetBrains ReSharper Ultimate.
      Default Silent Arguments:
      /VsVersion=14.0;15.0 /SpecificProductNames=dotCover;dotMemory;dotPeek;dotTrace;ReSharperCpp;teamCityAddin;ReSharper /Silent=True

      Override Silent Arguments:
      -ia '/VsVersion=14.0;15.0 /SpecificProductNames=ReSharper' -o
    </description>
    <owners>The-Running-Dev</owners>
		<packageSourceUrl>https://github.com/The-Running-Dev/Chocolatey-BoxStarter</packageSourceUrl>
    <dependencies>
      <dependency id="Chocolatey-Package.extension" />
    </dependencies>
  </metadata>
</package>