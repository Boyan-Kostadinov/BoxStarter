    <id>DisplayFusion</id>
    <version>9.0</version>
    <authors>Binary Fortress</authors>
    <title>DisplayFusion</title>
    <projectUrl>http://www.displayfusion.com</projectUrl>
    <iconUrl>https://github.com/logo.png</iconUrl>
    <licenseUrl>http://www.binaryfortress.com/data/download/?package=displayFusion&amp;licenseEN=1&amp;log=0</licenseUrl>
